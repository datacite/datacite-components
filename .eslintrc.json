{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
   "parserOptions": {
       "parser": "babel-eslint"
   },
  "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
  ],
  "rules": {
    "vue/no-v-html": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"

  }
}